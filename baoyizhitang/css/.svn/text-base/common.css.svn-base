/*mouse动画*/
@keyframes mouse{
	from{top:0px;}
	to{top:-10px}
}
@-moz-keyframes mouse{
	from{top:0px;}
	to{top:-10px}
}
@-webkit-keyframes mouse{
	from{top:0px;}
	to{top:-10px}
}
@-o-keyframes mouse{
	from{top:0px;}
	to{top:-10px}
}
.mouse {
  position: relative;
  width: 12%;
  left: 50%;
  max-width: 70px;
  cursor: pointer;
  transition: 0s;
  -moz-transition: 0s;
  -webkit-transition: 0s;
  -o-transition: 0s;
  z-index: 2;
   top: -30px;
  margin-left: -35px;
}
.mouse .bg {
  width: 100%;
}
.mouse .img {
  position: absolute;
  left: 0;
  top: 0;
  width: 100%;
  animation: mouse 1.5s infinite;
  -moz-animation: mouse 1.5s infinite;
  -webkit-animation: mouse 1.5s infinite;
  -o-animation: mouse 1.5s infinite;
}
.mouse img{
	z-index: 500;
	position: absolute;}	
  
