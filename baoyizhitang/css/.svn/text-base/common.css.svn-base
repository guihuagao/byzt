  /*图片滑动*/
 .flexslider {
	margin-top:20px;
	margin-bottom: 60px;
	position: relative;
	width: 100%;
	height: 450px;
	overflow: hidden;
	zoom: 1;
}

.flexslider .slides li {
	width: 100%;
	height: 400px;
}

.flex-direction-nav a {
	width: 70px;
	height: 70px;
	line-height: 99em;
	overflow: hidden;
	margin: -5px 0 0;
	display: block;
	background: url(../img/ad_tr.png) no-repeat;
	position: absolute;
	top: 50%;
	z-index: 10;
	cursor: pointer;
	opacity: 0;
	filter: alpha(opacity=1);
	-webkit-transition: all .3s ease;
	border-radius: 35px;
}
.flex-direction-nav .flex-next, .flex-direction-nav .flex-prev {
  position: absolute;
  z-index: 10;
  cursor: pointer;
  width: 43px;
  height: 35px;
  top: 50%;
  margin-top: -17px;
}
.flex-direction-nav .flex-next {
  margin-right: 60px;
  right: 3%;
  background: url(../img/ad_tr.png) right top;
}


.flex-direction-nav .flex-prev {
  margin-left: 60px;
  left: 3%;
  background: url(../img/ad_tr.png) left top;
}


.flexslider:hover .flex-next {
	opacity: 0.8;
	filter: alpha(opacity=25);
}

.flexslider:hover .flex-prev {
	opacity: 0.8;
	filter: alpha(opacity=25);
}

.flexslider:hover .flex-next:hover,
.flexslider:hover .flex-prev:hover {
	opacity: 1;
	filter: alpha(opacity=50);
}

.flex-control-nav {
	width: 100%;
	position: absolute;
	bottom: 10px;
	text-align: center;
	/*height: 400px;*/
}

.flex-control-nav li {
	margin: 0 2px;
	display: inline-block;
	zoom: 1;
	*display: inline;
}

.flex-control-paging li a {
	background: url(../img/dot.png) no-repeat 0 -16px;
	display: block;
	height: 16px;
	overflow: hidden;
	text-indent: -99em;
	width: 16px;
	cursor: pointer;
}

.flex-control-paging li a.flex-active,
.flex-control-paging li.active a {
	background-position: 0 0;
}

.flexslider .slides a img {
	width: 800px;
	height: 400px;
	display: block;
	margin: 50px auto;
} 
  
